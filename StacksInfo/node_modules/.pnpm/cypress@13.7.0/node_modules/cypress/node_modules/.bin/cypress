#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/StacksInfo/node_modules/.pnpm/cypress@13.7.0/node_modules/cypress/bin/node_modules:/mnt/c/StacksInfo/node_modules/.pnpm/cypress@13.7.0/node_modules/cypress/node_modules:/mnt/c/StacksInfo/node_modules/.pnpm/cypress@13.7.0/node_modules:/mnt/c/StacksInfo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/StacksInfo/node_modules/.pnpm/cypress@13.7.0/node_modules/cypress/bin/node_modules:/mnt/c/StacksInfo/node_modules/.pnpm/cypress@13.7.0/node_modules/cypress/node_modules:/mnt/c/StacksInfo/node_modules/.pnpm/cypress@13.7.0/node_modules:/mnt/c/StacksInfo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cypress" "$@"
else
  exec node  "$basedir/../../bin/cypress" "$@"
fi
